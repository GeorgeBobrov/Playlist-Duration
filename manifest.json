{
   "manifest_version": 2,
   "name": "Playlist Duration for YouTube",
   "description": "Shows the total duration of all videos on the playlist page",
   "version": "1.6",
   "content_scripts": [ {
      "css": [ "PlaylistDuration.css" ],
      "js": [ "PlaylistDuration.js" ],
      "matches": [ "https://www.youtube.com/*" ]
   } ],
   "background": {
      "scripts": ["background.js"],
      "persistent": false
   },
   "permissions": [
      "*://www.youtube.com/*", "tabs", "webNavigation"
   ],   
   "icons": {
      "128": "128x128.png",
      "16": "16x16.png",
      "32": "32x32.png",
      "48": "48x48.png"
   },
   "key": "",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "web_accessible_resources": [ "icon.svg" ]
}
